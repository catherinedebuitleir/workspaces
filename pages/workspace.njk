{# page variables #}
{% set master_page_location = "index.html" %}
{% set master_page_name = "Home" %}
{% set page_actions = false %}

{% set page_title = "AdAstraflex - Workspaces" %}

{# page layout #}
<head>
  {% block header %}
    {% include "./page_components/_head.html.njk" %}
  {% endblock %}
</head>
<body>
<div>

{# top menu #}
  {% include "./page_components/menus/_menu_top_cloud.njk" %}

  {# this is the side menu #}
  {% include "./page_components/menus/_menu_side_workspace.njk" %}

  

  <section class="gap"></section>
  <div class="container col-md-9 offset-md-2">
  <div class="shell-container">
    <div class="content-container"></div>

      <div class="row">
      <div class="col-md-8">
         <span class="breadcrumb-current-page">
              {{page_title}}
            </span>

        </div>

         <div class="col-md-4 f-r">
         <input type="text" class="form-control" id="searchworkspace" placeholder="Search workspaces">
         </div>
      <!-- This is where the page begins -->
        <div class="card" style="left: 25px;">

          {% block content %}

            {# any content goes here #}
              {% include "partials/_workspace_tiles.njk"%}
     

          {% endblock %}

        </div>
      </div>
    </div>

    {# {% include "./page_components/_footer.html.njk" %} #}
    {% include "./page_components/_scripts.html.njk" %}
</div>
</body>

